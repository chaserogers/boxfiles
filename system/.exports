export VISUAL=vim
export EDITOR="$VISUAL"
export FZF_DEFAULT_COMMAND="rg --hidden --files"
export RIPGREP_CONFIG_PATH="$HOME/.ripgreprc"

# matts whacky shit
export NVM_DIR="$HOME/.nvm"
