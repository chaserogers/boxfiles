alias ls='ls -lG'
alias ll='ls -laGh'
alias cl='clear'
alias vim='mvim -v'
alias e=$EDITOR

alias ..='cd ..'
alias work='cd ~/workspace'
alias notes='vim ~/notes'
alias dot='cd ~/.dotfiles/dotfiles && vim . && cd -'
